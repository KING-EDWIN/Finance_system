{
    "builds": [
        {
            "src": "clari6/wsgi.py",
            "use": "@vercel/python",
        "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src":"^/static/(.*)",
            "dest":"static/$1"

        },
        {
            "src": "/(.*)",
            "dest": "clari6/wsgi.py"
        }
    ]
}
